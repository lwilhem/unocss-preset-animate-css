{
  "name": "preset-uno-animate-css",
  "type": "module",
  "version": "1.0.0",
  "packageManager": "pnpm@10.15.0",
  "description": "A 1:1 implementation of tw-animate-css inside a unocss preset",
  "author": "Wilhem Lecanu <wilhem.lecanu@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/antfu/preset-uno-animate-css#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/antfu/preset-uno-animate-css.git"
  },
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "default": "./dist/index.mjs"
    }
  },
  "main": "dist/index.mjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "*.css",
    "*.d.ts",
    "dist"
  ],
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "check": "tsc --noEmit",
    "build": "unbuild"
  },
  "dependencies": {
    "@unocss/core": "catalog:unocss",
    "@unocss/extractor-arbitrary-variants": "catalog:unocss",
    "@unocss/rule-utils": "catalog:unocss"
  },
  "devDependencies": {
    "@antfu/eslint-config": "catalog:cli",
    "@types/node": "catalog:types",
    "eslint": "catalog:cli",
    "eslint-plugin-format": "catalog:cli",
    "typescript": "catalog:cli",
    "unbuild": "catalog:cli"
  }
}
